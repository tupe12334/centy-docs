{
  "name": "centy-docs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "generate:favicons": "tsx scripts/generate-favicons.ts",
    "i18n:check": "npx tsx scripts/check-translations.ts",
    "i18n:status": "npx tsx scripts/check-translations.ts --status",
    "i18n:extract": "docusaurus write-translations --locale en && docusaurus write-translations --locale es && docusaurus write-translations --locale fr && docusaurus write-translations --locale he",
    "pages:dev": "pnpm build && wrangler pages dev build",
    "pages:deploy": "pnpm build && wrangler pages deploy build"
  },
  "dependencies": {
    "@centy-io/assets": "^0.0.1",
    "@docusaurus/core": "3.9.2",
    "@docusaurus/preset-classic": "3.9.2",
    "@mdx-js/react": "^3.0.0",
    "clsx": "^2.0.0",
    "prism-react-renderer": "^2.3.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "3.9.2",
    "@docusaurus/tsconfig": "3.9.2",
    "@docusaurus/types": "3.9.2",
    "@types/node": "^20.0.0",
    "png-to-ico": "^3.0.1",
    "sharp": "^0.34.5",
    "tsx": "^4.0.0",
    "typescript": "~5.6.2",
    "wrangler": "^4.0.0"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 5 safari version"
    ]
  },
  "engines": {
    "node": ">=20.0"
  }
}
